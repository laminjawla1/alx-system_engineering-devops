server {
        listen 80 default_server;
        listen [::]:80 default_server;

        server_name 54.160.88.197
        add_header X-Served-By 374236-web-01;
        root /var/www/html;
        index index.html index.htm;

        location = /airbnb-onepage/ {
                proxy_pass http://127.0.0.1:5000/airbnb-onepaage/;
        }

        error_page 404 /custom_404.html;
        location /404 {
                root /var/www/html;
                internal;
        }
}
